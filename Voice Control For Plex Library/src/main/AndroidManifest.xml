<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atomjack.vcfplib">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:debuggable="false" >
        <activity android:name=".SetupActivity"
                  android:label="@string/app_name" >
            <intent-filter>
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="com.twofortyfouram.locale.intent.action.EDIT_SETTING" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.atomjack.vcfplib.PlayMediaActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
            </intent-filter>
        </activity>

        <service
            android:name="com.atomjack.vcfplib.GDMService"
            android:exported="false" />

        <receiver
            android:name="com.atomjack.vcfplib.GoogleSearchReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.mohammadag.googlesearchapi.NEW_SEARCH" />
            </intent-filter>
        </receiver>

        <receiver
            android:name="com.atomjack.vcfplib.PlayReceiver"
            android:exported="true"
            android:process=":background">
            <intent-filter>
                <action android:name="com.twofortyfouram.locale.intent.action.FIRE_SETTING" />
            </intent-filter>
        </receiver>
    </application>

</manifest>
